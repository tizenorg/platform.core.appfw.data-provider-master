<?xml version="1.0" encoding="utf-8" ?>
<manifest xmlns="http://tizen.org/ns/packages" package="com.samsung.nicesj" version="1.0" install-location="auto">

<livebox appid="com.samsung.nicesj.livebox" primary="true" secured="false" auto_launch="true" network="false" abi="c" timeout="20" period="10.0" libexec="/libexec/liblive-com.samsung.nicesj.so" pinup="false" script="edje">
	<label>My livebox</label>
	<icon>com.samsung.live-nicesj.png</icon>
	<label xml:lang="en_US">Sample</label>
	<label xml:lang="ko_KR">Hangul</label>

	<box type="image">
		<size>172x172</size>
		<size>348x172</size>
		<size>348x348</size>
		<size>700x348</size>
		<size>700x172</size>

		<script src="/res/script/my.edj" group="my_group" />
	</box>

	<pd type="script">
		<size>720x100</size>
		<script src="res/script/mypd.edj" group="group_string" />
	</pd>

	<group>
		<cluster name="cluster1">
			<category name="category1" context="APP_USED_FREQUENTLY">
				<option key="time_span" value="value" />
				<option key="result_size" value="value" />
			</category>

			<category name="category2" context="APP_USED_FREQUENTLY_THISTIME">
				<option key="time_span" value="value" />
				<option key="result_size" value="value" />
				<option key="time_interval" value="value" />
			</category>
		</cluster>
		<cluster name="cluster2">
			<category name="category1" context="CONTACT_COMMUNICATED_FREQUENTLY">
				<option key="data_provider" value="com.samsung.contact" />
				<option key="time_span" value="value" />
				<option key="result_size" value="size" />
			</category>
			<category name="category2" context="CONTACT_COMMUNICATED_FREQUENTLY_THISTIME">
				<option key="data_provider" value="com.samsung.contact" />
				<option key="time_span" value="value" />
				<option key="result_size" value="value" />
				<option key="time_interval" value="value" />
			</category>
			<category name="category3" context="CONTACT_COMMUNICATED_ATLEASTONCE">
				<option key="data_provider" value="value" />
				<option key="time_span" value="value" />
				<option key="result_size" value="value" />
			</category>
		</cluster>
		<!--
		     KEYWORD_SEARCHED_FREQUENTLY(DATA_PROVIDER, TIME_SPAN, RESULT_SIZE)
		     MUSIC_PLAYED_FREQUENTLY(DATA_PROVIDER, TIME_SPAN, RESULT_SIZE)
		-->
	</group>
</livebox>
</manifest>
